{
	"schemaVersion": 1,
	"id": "tooltrim",
	"version": "${version}",
	"name": "Tool Trims API",
	"description": "Library mod allowing players to add custom tool trims.",
	"authors": [
		"CobraGamingSJ"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/trim/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"org.cobra.api.tooltrim.ToolTrimsAPI"
		],
		"fabric-datagen": [
			"org.cobra.api.tooltrim.ToolTrimsAPIDataGenerator"
		]
	},
	"mixins": [
		"trim.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.4",
		"java": ">=21",
